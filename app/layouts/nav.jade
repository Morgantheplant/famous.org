nav.nav-main.cf(class=(pageType || ""))
  a.nav-logo(href='/')
    object(type="image/svg+xml" data="/images/famous-logo-text.svg")

  label.reveal-nav-btn(for='reveal-nav') Menu
  input.reveal-nav-input(type='checkbox' id='reveal-nav')

  .nav-main-list
    .list-left
      a.main-nav-link(href='/') Engine
      a.main-nav-link(href='/framework') Framework
    .list-right
      a.main-nav-link(href='/learn') Learn
      a.main-nav-link(href='/docs') API Docs
      a.main-nav-link(href='/support') Support
      a.main-nav-link(href='https://github.com/Famous/engine' onclick="trackOutboundLink('https://github.com/Famous/engine'); return false;") Github
      a.main-nav-link(href='https://blog.famous.org/') Blog

  block heading
  block subheading
  
block scripts
  script.
    /**
    * Function that tracks a click on an outbound link in Google Analytics.
    * This function takes a valid URL string as an argument, and uses that URL string
    * as the event label.
    */
    var trackOutboundLink = function(url) {
       ga('send', 'event', 'outbound', 'click', url, {'hitCallback':
         function () {
         document.location = url;
         }
       });
    }
